--- aggregates
-- count()
--- max(col)
    -- min(col)
    -- sum(col)
    -- avg(col)
    

 -- select count() as [total orders] from [order]


select c.categoryname as category, min(p.unitprice)
from [product] as p
join category as c
on p.categoryid = c.id
group by c.categoryname
order by p.unitprice